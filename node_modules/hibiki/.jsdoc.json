{
    "plugins": ["plugins/markdown"],
    "templates": {
        "default": {
            "outputSourceFiles": true
        }
    },
    "opts": {
        "recurse": true,
        "template": "./node_modules/docdash",
        "encoding": "utf8",
        "destination": "./docs/"
    },
    "source": {
        "include": ["./dist/"],
        "includePattern": ".+\\.js$"
    }
}